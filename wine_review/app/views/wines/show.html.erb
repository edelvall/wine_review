<h1>Wine Details</h1>
<p>Name: <%= @wine.name %> </p>
<p>Year: <%= @wine.year %> </p>
<p>Winery: <%= @wine.winery %> </p>
<p>Country: <%= @wine.country %> </p>
<p>Varietal: <%= @wine.varietal %> </p>
<%= link_to('All Wines', wines_path) %> | <%= link_to 'Edit', edit_wine_path(@wine) %>
<%= button_to 'Delete', @wine,
               method: :delete,
               data: { confirm: 'Are you sure?' } %>
